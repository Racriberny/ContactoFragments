<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="123dp"
        android:layout_height="117dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="287dp"
        android:layout_marginBottom="613dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/profile" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="288dp"
        android:layout_height="117dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="607dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/linearLayout"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/tvNombre"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:text="Nombre" />

        <TextView
            android:id="@+id/tvPrimero"
            android:layout_width="101dp"
            android:layout_height="wrap_content"
            android:text="Apellidos" />

        <TextView
            android:id="@+id/tvSegundo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Segundo" />

        <TextView
            android:id="@+id/tvDirecion"
            android:layout_width="101dp"
            android:layout_height="wrap_content"
            android:text="Direcion" />

        <TextView
            android:id="@+id/tvEmpresa"
            android:layout_width="101dp"
            android:layout_height="wrap_content"
            android:text="Empresa" />
    </LinearLayout>

    <TextView
        android:id="@+id/tvFecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="348dp"
        android:layout_marginBottom="579dp"
        android:text="Fecha"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

    <LinearLayout
        android:layout_width="232dp"
        android:layout_height="116dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="178dp"
        android:layout_marginBottom="461dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvFecha">

        <TextView
            android:id="@+id/contacto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Datos de contacto" />

        <TextView
            android:id="@+id/tvTelf1"
            android:layout_width="86dp"
            android:layout_height="wrap_content"
            android:text="Telefono 1" />

        <TextView
            android:id="@+id/tvTel2"
            android:layout_width="85dp"
            android:layout_height="wrap_content"
            android:text="Telefono 2" />

        <TextView
            android:id="@+id/tvEmail"
            android:layout_width="85dp"
            android:layout_height="wrap_content"
            android:text="Email" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>